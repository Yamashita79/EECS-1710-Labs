void setup()
{
  size(800,800);
  background(255);
}
void draw()
{
  if(mousePressed)
  {noStroke();
   fill(255,0,0);
   ellipse(mouseX-25,mouseY-25,10,10);
   fill(160,32,240);
   ellipse(mouseX+25,mouseY+25,10,10);  
   fill(255,165,0);
   ellipse(mouseX-15,mouseY-15,10,10);
   fill(0,0,255);
   ellipse(mouseX+15,mouseY+15,10,10);
   fill(255,255,0);
   ellipse(mouseX-5,mouseY-5,10,10);
   fill(34,139,34);
   ellipse(mouseX+5,mouseY+5,10,10);
  }
}
